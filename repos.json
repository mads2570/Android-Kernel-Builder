[  {
    "kernelSource": {
      "name": "mads-KernelSU",
      "repo": "https://github.com/mads2570/android_kernel_samsung_sm7225.git",
      "branch": "lineage-21.0",
      "device": "a42xq"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/proprietary-stuff/llvm-arm-toolchain-ship-10.0.git",
        "branch": "master",
        "name": "clang"
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "main",
        "name": "gcc"
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CROSS_COMPILE": "aarch64-linux-gnu-",
      "KERNEL_LLVM_BIN": "CLANG",
      "CLANG_TRIPLE": "aarch64-linux-gnu-",
      "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
      "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-",
      "KERNEL_MAKE_ENV" : "DTC_EXT=$(pwd)/tools/dtc CONFIG_BUILD_ARM64_DT_OVERLAY=y",
      "REAL_CC": "KERNEL_LLVM_BIN"
    }
  }
]
